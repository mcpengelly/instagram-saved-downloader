{
  "name": "instagram-saved-posts-images",
  "version": "1.0.0",
  "description": "Script for migrating saved posts from instagram to google photos",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc -p .",
    "runjs": "node ./lib/index.js",
    "lint": "tslint --project .",
    "refresh": "rm -rf node_modules && yarn install"
  },
  "author": "Matt Pengelly",
  "license": "MIT",
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/lodash": "^4.14.148",
    "@types/lodash.get": "^4.4.6",
    "@types/node-fetch": "^2.5.3",
    "@types/server-destroy": "^1.0.0",
    "axios": "^0.21.1",
    "fs-extra": "^8.1.0",
    "global": "^4.4.0",
    "google-auth-library": "^3.1.2",
    "googleapis": "^44.0.0",
    "googleapis-common": "^3.1.1",
    "googlephotos": "^0.2.4",
    "http": "^0.0.0",
    "inquirer": "^7.0.0",
    "instagram-private-api": "^1.13.0",
    "lodash": "^4.17.15",
    "node-fetch": "^3.1.1",
    "open": "^7.0.0",
    "server-destroy": "^1.0.1",
    "tslint": "^5.20.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^2.2.1",
    "ts-node": "^8.5.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
